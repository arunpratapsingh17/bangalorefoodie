<% include ../partial/header.ejs %>
<div class="container">
	<div class="row">
		<h1 style="text-align:center">
		Add A New Photo
		</h1>
		<div style="width:30%;margin:0 auto;">
			<form action="/campground" method="POST">
				<div class="form-group">
					<input type="text" class="form-control" name="name" placeholder="Name Please">
				</div>
				<div class="form-group">
				<input type="text" class="form-control" name="image" placeholder="image url">
				</div>
				<div class="form-group">
				<input type="text" class="form-control" name="description" placeholder="Write The Description">
				</div>
				<div class="form-group">
					<button class="btn btn-lg btn-primary btn-default btn-block">
						Submit
					</button>
				</div>
			</form>
			<a href="/campground">
				GO BACK
			</a>
		</div>
	</div>
</div>
<% include ../partial/footer.ejs %>